<!--pages/announcementDetail/announcementDetail.wxml-->
<swiper
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}"
  class="swiper-view"
  indicator-color="rgba(255,255,255,1)"
  indicator-active-color="rgba(10,187,8,1)"
>
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image lazy-load='true' mode="aspectFill" src="{{item}}" class="swiper-view" />
    </swiper-item>
  </block>
</swiper>

